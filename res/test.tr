// Pedantic function decleration.
let triplePedantic: (int)->(int) = (x: int) -> (int) {
  return 3 * x;
}

// Inferred function decleration.
let tripleInferred = (x: int) -> (int) {
  return 3 * x;
}

// Higher order function.
let higher = (x: int, f: (int)->(int)) -> (int) {
  return f(x);
}

// The main function
let main = () -> () {
  let result = higher(4, tripleInferred);
}
